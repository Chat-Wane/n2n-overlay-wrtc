{
    "name": "n2n-signaling-wrtc",
    "version": "0.0.1",
    "description": "A WebRTC peer is able to mediate the connection establishment between its neighbors, i.e., it becomes a signaling server for its neighbors.",
    "main": "lib/n2n-signaling.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/chat-wane/n2n-signaling-wrtc.git/"
    },
    "keywords": [
        "WebRTC",
        "browser-to-browser communication",
        "overlay network",
        "distributed signaling server"
    ],
    "author": "Chat-Wane",
    "license": "MIT",
    "dependencies": {
        "neighborhood-wrtc": "0.0.x",
        "sorted-cmp-array": "^2.0.1"
    },
    "bundledDependencies": [
        "neighborhood-wrtc",
        "sorted-cmp-array"
    ],
    "devDependencies": {
        "browserify": "6.2.x"
    },
    "scripts":{
        "debug": "browserify -r ./lib/n2n-signaling.js:n2n-signaling-wrtc --debug > build/n2n-signaling-wrtc.bundle.debug.js",
        "build": "browserify -r ./lib/n2n-signaling.js:n2n-signaling-wrtc | uglifyjs > build/n2n-signaling-wrtc.bundle.js"
    }
}
